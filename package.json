{
  "name": "koa-react-note-manager",
  "version": "0.0.1",
  "description": "Project done with Koa &amp; React/Redux in order to manage note application.",
  "main": "server/index.js",
  "dependencies": {
    "koa": "^2.5.2",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-react-flow-props-to-prop-types": "^0.15.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-airbnb": "^2.5.2",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.77.0",
    "mocha": "^5.2.0",
    "mockgoose": "^7.3.5",
    "mongoose-mock": "^0.4.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "prop-types": "^15.6.2",
    "proxyquire": "^2.0.1"
  },
  "scripts": {
    "lint:server": "eslint server/ && flow server/",
    "lint:client": "eslint client/ && flow client/",
    "lint": "npm run lint:server",
    "test:server": "mocha --exit --timeout 120000 --reporter progress --require babel-core/register server/**/*.spec.js",
    "test:client": "mocha --exit --timeout 120000 --reporter progress --require babel-core/register client/**/*.spec.js",
    "test": "npm run test:server",
    "start:dev": "nodemon server/index.js --exec babel-node",
    "coverage:server": "nyc --reporter=lcov -- npm run test:server",
    "coverage:client": "nyc --reporter=lcov -- npm run test:client",
    "coverage:report": "cat ./coverage/lcov.info | coveralls",
    "coverage": "coverage:server",
    "coverage:clear": "rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karudedios/koa-react-note-manager.git"
  },
  "author": "Carlos de Dios <c.dedios@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karudedios/koa-react-note-manager/issues"
  },
  "homepage": "https://github.com/karudedios/koa-react-note-manager#readme"
}
